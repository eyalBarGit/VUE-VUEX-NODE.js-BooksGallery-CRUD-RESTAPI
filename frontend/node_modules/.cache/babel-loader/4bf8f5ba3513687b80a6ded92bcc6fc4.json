AEA,IAAA,SAAA,EAAA;AAFA,GAVA;AAcA,EAAA,OAAA,EAAA;AACA,IAAA,SADA,qBACA,MADA,EACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,KAAA,CAAA,qBAAA;;AADA;AAAA,uBAEA,WAAA,CAAA,SAAA,CAAA,MAAA,CAFA;;AAAA;AAGA,gBAAA,KAAA,CAAA,UAAA,GAAA,KAAA;;AACA,gBAAA,KAAA,CAAA,UAAA;;AAJA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KANA;AAOA,IAAA,YAPA,wBAOA,MAPA,EAOA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,qBAAA;;AADA;AAAA,uBAEA,WAAA,CAAA,YAAA,CAAA,MAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,UAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KAXA;AAYA,IAAA,UAZA,wBAYA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MADA,GACA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,EADA;AAAA;AAAA,uBAEA,WAAA,CAAA,OAAA,CAAA,MAAA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,MAAA,CAAA,IAAA,GAAA,QAAA;;AACA,gBAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,gBAAA,EAAA,MAAA,CAAA,IAAA;;AAJA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KAjBA;AAkBA,IAAA,qBAlBA,mCAkBA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,sBAAA,EAAA,IAAA;AACA,KApBA;AAqBA,IAAA,QArBA,sBAqBA;AACA,aAAA,KAAA,CAAA,KAAA,IAAA,CAAA,aAAA,CAAA,GAAA,KAAA,GAAA,IAAA;AACA,KAvBA;AAwBA,IAAA,YAxBA,0BAwBA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,aADA;AAEA,QAAA,MAAA,EAAA;AACA,UAAA,EAAA,EAAA,KAAA,MAAA,CAAA,OAAA,CAAA;AADA;AAFA,OAAA;AAMA;AA/BA,GAdA;AA+CA,EAAA,KAAA,EAAA;AACA,sBADA,0BACA,MADA,EACA,MADA,EACA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA,EAAA,MAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA,EAAA,MAAA;AACA,UAAA,MAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA;AACA,MAAA,WAAA,CAAA,OAAA,CAAA,MAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,IAAA;AACA,OAFA;AAGA;AARA,GA/CA;AAyDA,EAAA,QAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,UAAA,SAAA,GAAA,KAAA,IAAA,CAAA,SAAA;;AACA,UAAA,SAAA,GAAA,GAAA,EAAA;AACA,eAAA,cAAA;AACA;;AACA,UAAA,SAAA,GAAA,GAAA,EAAA;AACA,eAAA,gBAAA;AACA;;AACA,UAAA,SAAA,GAAA,GAAA,EAAA;AACA,eAAA,eAAA;AACA;;AACA,aAAA,gBAAA;AACA,KAbA;AAcA,IAAA,OAdA,qBAcA;AACA,UAAA,OAAA,GAAA,KAAA,IAAA,CAAA,aAAA;AACA,UAAA,QAAA,GAAA,IAAA,IAAA,GAAA,WAAA,EAAA;AACA,UAAA,eAAA,GAAA,QAAA,GAAA,OAAA;AACA,UAAA,eAAA,GAAA,EAAA,EAAA,OAAA,SAAA;AACA,UAAA,eAAA,GAAA,CAAA,EAAA,OAAA,KAAA;AACA,aAAA,eAAA,GAAA,YAAA;AACA;AArBA,GAzDA;AAgFA,EAAA,OAhFA,qBAgFA,CAAA,CAhFA;AAiFA,EAAA,OAjFA,qBAiFA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,cAAA,MAAA,CAAA,UAAA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA;AAnFA,CAAA","sourcesContent":["<template>\r\n  <p v-if=\"!book\">loading...</p>\r\n  <div v-else class=\"bookDetails d-flex flex-column align-center justify-space-around\">\r\n    <v-card class=\"mx-auto my-12\" max-width=\"800\" width=\"100%\">\r\n      <v-img height=\"300\" :src=\"book.thumbnail\">\r\n        <v-chip v-if=\"book.listPrice.isOnSale\" class=\"ma-4\" color=\"red\" text-color=\"white\">\r\n          <v-avatar left>\r\n            <v-icon>mdi-sale</v-icon>\r\n          </v-avatar>\r\n          <span class=\"font-weight-bold subtitle-1\">On Sale!</span>\r\n        </v-chip>\r\n      </v-img>\r\n      <v-card-title>\r\n        {{ book.title.toUpperCase() }}\r\n        <v-spacer></v-spacer>\r\n        <div v-if=\"checkNan()\">{{ bookAge }}</div>\r\n      </v-card-title>\r\n      <v-card-subtitle v-if=\"book.authors\">\r\n        <span class=\"font-weight-bold\">Authors:</span>\r\n        {{ book.authors.join(\" \") }}\r\n        <div v-if=\"book.categories\">\r\n          <span class=\"font-weight-bold\">Categories:</span>\r\n          {{ book.categories.join(\" \") }}\r\n        </div>\r\n      </v-card-subtitle>\r\n      <v-card-text v-if=\"book.description\">\r\n        <span class=\"font-weight-bold\">Description:</span>\r\n        <long-text :txt=\"book.description\"></long-text>\r\n        <div>\r\n          <span class=\"font-weight-bold\">Language:</span>\r\n          {{ book.language }}\r\n        </div>\r\n      </v-card-text>\r\n\r\n      <v-card-actions>\r\n        <span>{{ readStatus }}</span>\r\n        <v-spacer></v-spacer>\r\n        <span class=\"font-weight-bold\">Price:</span>\r\n        <span\r\n          class=\"font-weight-bold\"\r\n          :class=\"{\r\n            redColor: book.listPrice.amount > 150,\r\n            greenColor: book.listPrice.amount < 20,\r\n          }\"\r\n        >{{ book.listPrice.amount }}</span>\r\n      </v-card-actions>\r\n    </v-card>\r\n    <div class=\"reviews-container\">\r\n      <div class=\"add-review-container d-flex justify-center\">\r\n        <div>\r\n          <v-btn @click=\"showReview = !showReview\">Add-Review</v-btn>\r\n        </div>\r\n        <addReview :bookIdKey=\"book.id\" @addReview=\"addReview\" v-if=\"showReview\"></addReview>\r\n      </div>\r\n      <div v-if=\"book.reviews\">\r\n        <div\r\n          class=\"review-card d-flex justify-space-between\"\r\n          :key=\"idx\"\r\n          v-for=\"(review, idx) in book.reviews\"\r\n        >\r\n          <div class=\"d-flex flex-column\">\r\n            <div class=\"name-date d-flex justify-space-between\">\r\n              <div class=\"d-flex\">\r\n                <p class=\"bold\">name:</p>\r\n                <p>{{ review.name }}</p>\r\n              </div>\r\n              <div class=\"d-flex\">\r\n                <p class=\"bold\">date:</p>\r\n                <p>{{ review.date }}</p>\r\n              </div>\r\n            </div>\r\n            <div class=\"d-flex\">\r\n              <p class=\"bold\">review:</p>\r\n              <p>{{ review.txt }}</p>\r\n            </div>\r\n          </div>\r\n          <div class=\"div\">\r\n            <v-btn @click=\"removeReview(review)\">X</v-btn>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <v-btn @click=\"goToNextBook\">Next-Book</v-btn>\r\n  </div>\r\n</template>\r\n<script>\r\nimport longText from \"../components/longText\";\r\nimport addReview from \"../components/addReview\";\r\nimport bookService from \"../services/bookService.js\";\r\nexport default {\r\n  name: \"bookDetails\",\r\n  data() {\r\n    return {\r\n      book: null,\r\n      showReview: false,\r\n      // reviews: [],\r\n      nextBook: false\r\n    };\r\n  },\r\n  components: {\r\n    longText,\r\n    addReview\r\n  },\r\n  methods: {\r\n    async addReview(review) {\r\n      this.setNotificationActive();\r\n      await bookService.addReview(review);\r\n      this.showReview = false;\r\n      this.updateBook();\r\n    },\r\n    async removeReview(review) {\r\n      this.setNotificationActive();\r\n      await bookService.removeReview(review);\r\n      this.updateBook();\r\n    },\r\n    async updateBook() {\r\n      let bookId = this.$route.params.id;\r\n      const currBook = await bookService.getById(bookId);\r\n      this.book = currBook;\r\n      this.$store.dispatch(\"updateCurrBook\", this.book);\r\n    },\r\n    setNotificationActive() {\r\n      this.$store.dispatch(\"updateSnackBarStatus\", true);\r\n    },\r\n    checkNan() {\r\n      return isNaN(this.book.publishedDate) ? false : true;\r\n    },\r\n      goToNextBook() {\r\n      this.$router.push({\r\n        name: \"bookDetails\",\r\n        params: {\r\n          id: this.$store.getters.getNextBook\r\n        }\r\n      });\r\n    }\r\n  },\r\n  watch: {\r\n    \"$route.params.id\"(newVal, oldVal) {\r\n      console.log(\"newVal\", newVal);\r\n      console.log(\"oldVal\", oldVal);\r\n      let bookId = this.$route.params.id;\r\n      bookService.getById(bookId).then(book => {\r\n        this.book = book;\r\n      });\r\n    }\r\n  },\r\n  computed: {\r\n    readStatus() {\r\n      let pageCount = this.book.pageCount;\r\n      if (pageCount > 500) {\r\n        return \"Long reading\";\r\n      }\r\n      if (pageCount > 200) {\r\n        return \"Decent Reading\";\r\n      }\r\n      if (pageCount < 100) {\r\n        return \"Light Reading\";\r\n      }\r\n      return \"Medium Reading\";\r\n    },\r\n    bookAge() {\r\n      let bookAge = this.book.publishedDate;\r\n      let currYear = new Date().getFullYear();\r\n      let yearsDifference = currYear - bookAge;\r\n      if (yearsDifference > 10) return \"Veteran\";\r\n      if (yearsDifference < 1) return \"New\";\r\n      return yearsDifference + \" years ago\";\r\n    }\r\n  },\r\n  mounted() {},\r\n  async created() {\r\n    this.updateBook();\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\">\r\n.bold {\r\n  font-weight: 700;\r\n}\r\n.reviews-container {\r\n  width: 500px;\r\n  .add-review-container {\r\n    width: 100%;\r\n    margin-bottom: 20px;\r\n  }\r\n}\r\n\r\n.review-card {\r\n  margin: 15px 0;\r\n  width: 500px;\r\n  padding: 20px;\r\n  background-color: lightgray;\r\n  .name-date {\r\n    width: 400px;\r\n    border-bottom: 1px solid rgb(255, 255, 255);\r\n    padding-right: 20px;\r\n  }\r\n}\r\n.greenColor {\r\n  color: green;\r\n}\r\n.redColor {\r\n  color: red;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}